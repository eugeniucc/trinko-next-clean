generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model HomepageSketches {
  id        Int      @id @default(autoincrement())
  title     String
  url       String
  alt       String
  createdAt DateTime @default(now())
}

model Portfolio {
  id        Int      @id @default(autoincrement())
  title     String
  url       String
  alt       String
  createdAt DateTime @default(now())
}
