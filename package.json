  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate:supabase": "prisma generate --schema=prisma/supabase.schema.prisma",
    "prisma:migrate:supabase": "prisma migrate dev --schema=prisma/supabase.schema.prisma",
    "prisma:generate:neon": "prisma generate --schema=prisma/neon.schema.prisma",
    "prisma:migrate:neon": "prisma migrate dev --schema=prisma/neon.schema.prisma",
    "prisma:generate:all": "npm run prisma:generate:supabase && npm run prisma:generate:neon",
    "postinstall": "npm run prisma:generate:all"
  },
